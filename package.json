{
  "name": "master-workers-sqs",
  "version": "2.0.0",
  "description": "Fault-tolerant distributed worker-consumer system with Node.js, MongoDB, and AWS SQS",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=development MODE=MW node server.js",
    "start:master": "NODE_ENV=development MODE=M node server.js",
    "start:worker": "NODE_ENV=development MODE=W node server.js",
    "dev": "nodemon server.js",
    "test": "mocha __tests__/**/*.test.js --timeout 10000",
    "setup": "./start-services.sh",
    "cleanup": "./stop-services.sh",
    "queue:create": "node sqs.js create",
    "queue:list": "node sqs.js list",
    "queue:delete": "node sqs.js delete",
    "db:status": "mongosh masterworkers --eval 'db.stats()'",
    "db:jobs": "mongosh masterworkers --eval 'db.jobs.find().sort({createdAt: -1}).limit(5).pretty()'",
    "db:queues": "mongosh masterworkers --eval 'db.queues.find().pretty()'"
  },
  "keywords": [
    "nodejs",
    "mongodb", 
    "aws-sqs",
    "localstack",
    "distributed-systems",
    "message-queue",
    "worker-pool",
    "microservices"
  ],
  "author": "Master-Workers Team",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-sqs": "^3.864.0",
    "async": "^3.2.6",
    "dotenv": "16.4.7",
    "express": "5.1.0",
    "mongodb": "6.15.0",
    "nconf": "0.12.1"
  },
  "devDependencies": {
    "mocha": "^10.0.0",
    "nodemon": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vishwas411/master-workers-sqs.git"
  }
}